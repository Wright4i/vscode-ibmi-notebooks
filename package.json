{
	"name": "vscode-ibmi-notebooks",
	"displayName": "vscode-ibmi-notebooks",
	"description": "VS Code Notebooks for IBM i",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"author": {
		"name": "Halcyon Tech Ltd",
		"url": "https://github.com/halcyon-tech"
	},
	"publisher": "halcyontechltd",
	"extensionDependencies": [
		"halcyontechltd.code-for-ibmi"
	],
	"categories": [
		"Notebooks"
	],
	"activationEvents": [
		"onNotebook:ibmi-notebook"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"notebooks": [
			{
				"id": "ibmi-notebook",
				"type": "ibmi-notebook",
				"displayName": "IBM i Notebook",
				"selector": [
					{
						"filenamePattern": "*.inb"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"json-to-markdown-table": "^1.0.0"
	}
}
